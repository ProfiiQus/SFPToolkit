{
	"Generates a new TextBox": {
		"prefix": "textbox",
		"body": [
		  "<Control xsi:type=\"TextBoxControl\" Ident=\"$1\" DataType=\"${2|String,Number,Bool|}\" IsVisible=\"${3|true,false|}\" IsReadOnly=\"${4|false,true|}\" TitleResourceKey=\"$1_$5\" />",
		  "$0"
		],
		"description": "Generates a new TextBox"
	},
