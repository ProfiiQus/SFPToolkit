	"Generates a new RichTextBoxControl": {
		"prefix": "richtextbox",
		"body": [
		  "<Control xsi:type=\"RichTextBoxControl\" Ident=\"$1\" Height=\"${2:350}\" DataType=\"String\" IsVisible=\"${3|true,false|}\" IsReadOnly=\"${4|false,true|}\" TitleResourceKey=\"$1_$5\" />",
		  "$0"
		],
		"description": "Generates a new RichTextBoxControl"
	},
	"Generates a new RichTextBoxControl with defined ToolBarSet": {
		"prefix": "richtextbox-toolbar",
		"body": [
		  "<!-- https://ckeditor.com/latest/samples/toolbarconfigurator/index.html#basic -->",
		  "<Control xsi:type=\"RichTextBoxControl\" Ident=\"$1\" Height=\"${2:350}\" DataType=\"String\" IsVisible=\"${3|true,false|}\" IsReadOnly=\"${4|false,true|}\" IsFileManager=\"${5|false,true|}\" TitleResourceKey=\"$1_$6\" >",
		  "    <ToolBarSet>",
		  "        [",
		  "            { name: 'document', groups: ['mode', 'document', 'doctools'], items: ['Source', '-', 'NewPage'] },",
		  "            { name: 'clipboard', groups: ['clipboard', 'undo'], items: ['Cut', 'Copy', 'Paste', 'PasteText', 'PasteFromWord', '-', 'Undo', 'Redo'] },",
		  "            { name: 'editing', groups: ['find', 'selection', 'spellchecker'], items: ['Find', 'Replace', '-', 'SelectAll', '-', 'Scayt'] },",
		  "            { name: 'basicstyles', groups: ['basicstyles', 'cleanup'], items: ['Bold', 'Italic', 'Underline', 'Strike', 'Subscript', 'Superscript', '-', 'RemoveFormat'] },",
		  "            { name: 'paragraph', groups: ['list', 'indent', 'blocks', 'align', 'bidi'], items: ['NumberedList', 'BulletedList', '-', 'Outdent', 'Indent', '-', 'Blockquote', 'CreateDiv', '-', 'JustifyLeft', 'JustifyCenter', 'JustifyRight', 'JustifyBlock', '-', 'BidiLtr', 'BidiRtl'] },",
		  "            { name: 'links', items: ['Link', 'Unlink', 'Anchor'] },",
		  "            { name: 'insert', items: ['Image', 'Flash', 'Table', 'HorizontalRule', 'Smiley', 'SpecialChar', 'PageBreak', 'Iframe'] },",
		  "            { name: 'styles', items: ['Styles', 'Format', 'Font', 'FontSize'] },",
		  "            { name: 'colors', items: ['TextColor', 'BGColor'] },",
		  "            { name: 'tools', items: ['Maximize', 'ShowBlocks'] },",
		  "            { name: 'others', items: ['-'] }",
		  "        ]",
		  "    </ToolBarSet>",
		  "</Control>",
		  "$0"
		],
		"description": "Generates a new RichTextBoxControl with defined ToolBarSet"
	},
	"Generates a new RadioButtonListControl": {
		"prefix": "radiobuttonlist",
		"body": [
		  "<Control xsi:type=\"RadioButtonListControl\" Ident=\"$1\" DataType=\"Number\" DisplayType=\"Inline\" TitleResourceKey=\"$1_$2\">",
		  "    <ListItems>",
		  "        <ListItem TitleResourceKey=\"_$2\" Value=\"1\" />",
		  "        <ListItem TitleResourceKey=\"_$2\" Value=\"2\" />",
		  "        <ListItem TitleResourceKey=\"_$2\" Value=\"3\" />",
		  "    </ListItems>",
		  "</Control>"
		],
		"description": "Generates a new RadioButtonListControl"
	},
	"Generates a new AutoCompleteControl": {
		"prefix": "autocomplete",
		"body": [
		  "<Control xsi:type=\"AutoCompleteControl\" Ident=\"$1\" DataType=\"Number\" TitleResourceKey=\"$1_$2\" IsVisible=\"${3|true,false|}\" IsReadOnly=\"${4|false,true|}\" >",
		  "    <DataBind DefaultTitleResourceKey=\"SelectValue\">",
		  "        <Columns>",
		  "            <Column Ident=\"ID\" TitleResourceKey=\"ID_$2\" DataBindType=\"Value\" />",
		  "            <Column Ident=\"Title\" TitleResourceKey=\"Title_$2\" DataBindType=\"Title\" />",
		  "        </Columns>",
		  "        <SQL>",
		  "            SELECT ID, FullName FROM dbo.Account WHERE FullName like @$1",
		  "            $0",
		  "        </SQL>",
		  "        <Parameters>",
		  "            <dsp:Parameter xsi:type=\"dsp:VariableParameter\" Ident=\"$1\" DataType=\"String\" LikeType=\"Both\" MaxLenght=\"200\" />",
		  "        </Parameters>",
		  "    </DataBind>",
		  "    <SelectedDataBind>",
		  "        <Columns>",
		  "            <Column Ident=\"ID\" TitleResourceKey=\"ID_$2\" DataBindType=\"Value\" />",
		  "            <Column Ident=\"Title\" TitleResourceKey=\"ID_$2\" DataBindType=\"Title\" />",
		  "        </Columns>",
		  "        <SQL>",
		  "            SELECT ID, FullName FROM dbo.Account WHERE ID = @$1",
		  "        </SQL>",
		  "        <Parameters>",
		  "            <dsp:Parameter xsi:type=\"dsp:VariableParameter\" Ident=\"$1\" DataType=\"Number\" />",
		  "        </Parameters>",
		  "    </SelectedDataBind>",
		  "</Control>"
		],
		"description": "Generates a new AutoCompleteControl"
	},
	"Generates a new DropDownListControl": {
		"prefix": "dropdown",
		"body": [
		  "<Control xsi:type=\"DropDownListControl\" Ident=\"$1\" DataType=\"Number\" TitleResourceKey=\"$1_$2\">",
		  "    <ListItems>",
		  "        <ListItem TitleResourceKey=\"$3_$2\" Value=\"1\" />",
		  "        <ListItem TitleResourceKey=\"$4_$2\" Value=\"2\" />",
		  "        <ListItem TitleResourceKey=\"$5_$2\" Value=\"3\" />",
		  "    </ListItems>",
		  "</Control>",
		  "$0"
		],
		"description": "Generates a new DropDownListControl"
	},
