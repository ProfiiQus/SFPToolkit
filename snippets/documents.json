      "			<HTMLTemplate>",
      "				<div class=\"card card-body bg-danger-400 has-bg-image\">",
      "					<div class=\"media mb-3\">",
      "						<div class=\"media-body\">",
      "							<h6 class=\"font-weight-semibold mb-0\">$0Přehled detailů formuláře</h6>",
